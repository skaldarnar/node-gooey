{
  "name": "gooey",
  "version": "0.2.0",
  "description": "CLI tool for working with Terasology workspaces.",
  "author": "Tobias Nett @skaldarnar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skaldarnar/node-gooey"
  },
  "bugs": "https://github.com/skaldarnar/node-gooey/issues",
  "homepage": "https://github.com/skaldarnar/node-gooey#readme",
  "bin": {
    "gooey": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "package": "oclif pack tarballs && oclif pack win && oclif pack macos && oclif pack deb",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@octokit/rest": "^18.12.0",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "dot-properties": "^1.0.1",
    "execa": "^5.1.1",
    "fs-extra": "^10.1.0",
    "ora": "^6.1.0",
    "semver": "^7.3.7",
    "simple-git": "^3.7.1",
    "tiny-async-pool": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/semver": "^7.3.9",
    "@types/tiny-async-pool": "^2.0.0",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "types": "dist/index.d.ts",
  "oclif": {
    "bin": "gooey",
    "dirname": "gooey",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "workspace": {
        "description": "Manage a Terasology workspace."
      },
      "module": {
        "description": "Manage a module and its dependencies and dependants."
      }
    }
  }
}
